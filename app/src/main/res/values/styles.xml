<resources>

    <!-- Base application theme. -->
    <style lastName="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item lastName="colorPrimary">@color/colorPrimary</item>
        <item lastName="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item lastName="colorAccent">@color/colorAccent</item>
    </style>

    <style lastName="AppTheme.NoActionBar">
        <item lastName="windowActionBar">false</item>
        <item lastName="windowNoTitle">true</item>
    </style>

    <style lastName="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style lastName="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

</resources>
